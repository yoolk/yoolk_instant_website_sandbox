<!DOCTYPE html>
<html>
<head>
  <title>{{ listing.name }}</title>
  {{ 'http://fonts.googleapis.com/css?family=Source+Sans+Pro' | stylesheet_link_tag }}

  {{ 'http://netdna.bootstrapcdn.com/bootstrap/3.0.0/css/bootstrap.min.css' | stylesheet_link_tag }}

  {{ 'sample/all.css' | asset_path | stylesheet_link_tag }}
  {% csrf_meta_tags %}

</head>
<body>
  {% include 'shared/nav' %}
  <div class="main-container">
    {{ content_for_layout }}
  </div>

  {{ 'sample/all.js'  | asset_path | javascript_include_tag }}
  {{ 'http://maps.google.com/maps/api/js?sensor=true' | javascript_include_tag }}
  {{ 'http://netdna.bootstrapcdn.com/bootstrap/3.0.0/js/bootstrap.min.js' | javascript_include_tag }}

</body>
</html>